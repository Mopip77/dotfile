{
  "title": "v75键盘配置",
  "desciprtion": "v75键盘旋钮分别是音量加减和(ctrl+shift+f9)，下面的四个按键依次是insert，end，page_up，page_down, 简称为r1-r4",
  "rules": [
    {
      "description": "v75键盘配置",
      "manipulators": [
        {
          "description": "r1+r2 进入高级层",
          "type": "basic",
          "conditions": [
            {
              "name": "advanced_layer",
              "type": "variable_unless",
              "value": 1
            },
            {
              "type": "device_if",
              "identifiers": [
                {
                  "product_id": 64020,
                  "description": "v75"
                },
                {
                  "product_id": 64023,
                  "description": "v75-2.4g"
                }
              ]
            }
          ],
          "from": {
            "simultaneous": [
              {
                "key_code": "insert"
              },
              {
                "key_code": "end"
              }
            ]
          },
          "to": [
            {
              "set_variable": {
                "name": "advanced_layer",
                "value": 1
              }
            },
            {
              "shell_command": "osascript -e 'display notification \"进入高级层\"'"
            }
          ]
        },
        {
          "description": "r1+r2 退出高级层",
          "type": "basic",
          "conditions": [
            {
              "name": "advanced_layer",
              "type": "variable_unless",
              "value": 0
            },
            {
              "type": "device_if",
              "identifiers": [
                {
                  "product_id": 64020,
                  "description": "v75"
                },
                {
                  "product_id": 64023,
                  "description": "v75-2.4g"
                }
              ]
            }
          ],
          "from": {
            "simultaneous": [
              {
                "key_code": "insert"
              },
              {
                "key_code": "end"
              }
            ]
          },
          "to": [
            {
              "set_variable": {
                "name": "advanced_layer",
                "value": 0
              }
            },
            {
              "shell_command": "osascript -e 'display notification \"退出高级层\"'"
            }
          ]
        },
        {
          "description": "旋钮按下 => 静音",
