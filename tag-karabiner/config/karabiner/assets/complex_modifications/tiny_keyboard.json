              },
              {
                "key_code": "f18"
              }
            ]
          },
          "to": [
            {
              "set_variable": {
                "name": "advanced_layer",
                "value": 0
              }
            },
            {
              "shell_command": "osascript -e 'display notification \"退出高级层\"'"
            }
          ]
        },
