{
  "title": "小键盘替换按键",
  "rules": [
    {
      "description": "小键盘替换",
      "manipulators": [
        {
          "description": "layer2 1+3 进入高级层",
          "type": "basic",
          "conditions": [
            {
              "name": "advanced_layer",
              "type": "variable_unless",
              "value": 1
            },
            {
              "type": "device_if",
              "identifiers": [
                {
                  "product_id": 8455,
                  "description": "tiny_keyboard"
                }
              ]
            }
          ],
          "from": {
            "simultaneous": [
              {
                "key_code": "f16"
              },
              {
                "key_code": "f18"
              }
            ]
          },
          "to": [
            {
              "set_variable": {
                "name": "advanced_layer",
                "value": 1
              }
            },
            {
              "shell_command": "osascript -e 'display notification \"进入高级层\"'"
            }
          ]
        },
        {
          "description": "layer2 1+3 退出高级层",
          "type": "basic",
          "conditions": [
            {
              "name": "advanced_layer",
              "type": "variable_unless",
              "value": 0
            },
            {
              "type": "device_if",
              "identifiers": [
                {
